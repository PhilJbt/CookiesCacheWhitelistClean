{
  "manifest_version": 3,
  "name": "__MSG_extTitle__",
  "description": "__MSG_extDescription__",
  "version": "1.1",
  "default_locale": "en",
  "permissions": [
    "storage",
    "browsingData",
    "cookies",
    "tabs",
    "downloads",
		"notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "img/icn16.png",
    "48": "img/icn48.png",
    "128": "img/icn128.png"
  },
  "action": {
    "default_popup": "options.html?type=popup"
  },
  "options_page": "options.html"
}
